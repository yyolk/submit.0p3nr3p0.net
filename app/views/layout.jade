doctype html
html(lang="en")
  head
    meta(charset="utf-8")
    title Produce Consume : NOTHING BETWEEN US
    block css
      != css('styles.css')

  block body
    body(data-view="#{view}")
      block content

      block js
        | <script>
        | document.write('<script src=' +
        | ('__proto__' in {} ? 'http://zeptojs.com/zepto.min.js' : 'http://code.jquery.com/jquery-1.7.2.min.js') +
        | '><\/script>')
        | </script>

        != js('scripts.js')
